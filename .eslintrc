{
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "none",
                "singleQuote": true,
                "tabWidth": 2,
                "printWidth": 100
            }
        ]
    },
    "extends": ["prettier"],
    "plugins": ["prettier"]
}